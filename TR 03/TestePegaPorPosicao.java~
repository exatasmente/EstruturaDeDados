public class TestePegaPorPosicao {
  public static void main(String[] args) {
    ListaEncadeada lista = new ListaEncadeada();
    
        
    lista.addFirst("Rafael");
    lista.addLast("Paulo");
    lista.addFirst("Camila");
    
    lista.remove(1);
    
    System.out.println(lista);
  }
  }

